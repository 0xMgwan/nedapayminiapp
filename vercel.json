{
  "buildCommand": "cd .. && npm install --omit=dev && npm run build",
  "devCommand": "next dev",
  "installCommand": "cd .. && npm install --omit=dev",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "workspaces": [
    "."
  ],
  "ignoreBuildScripts": true,
  "regions": ["iad1"],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.html" }
  ]
}
